{
  "name": "vercel-typescript-express-api",
  "version": "0.0.2",
  "description": "REST API example to deploy in Vercel(Typescript, Express. MongoDB)",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "pre-prod": "rimraf dist && npm run build",
    "dev": "nodemon --watch src --exec ts-node src/server.ts",
    "predev": "npm run build",
    "prepare:enviroment": "node scripts/dev-enviroment.cjs",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "engines": {
    "node": ">=16.18",
    "npm": ">=8.18"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.2",
    "@vercel/node": "^2.15.6",
    "awilix": "^8.0.1",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "morgan": "^1.10.0",
    "nest-shared": "^5.0.2",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.12",
    "nodemailer": "^6.9.5",
    "request-ip": "^3.3.0",
    "rimraf": "^5.0.1",
    "ytdl-core": "^4.11.5",
    "ytpl": "^2.3.0"
  },
  "devDependencies": {
    "@types/nodemailer": "^6.4.9",
    "@types/request-ip": "^0.0.38",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest-cucumber": "^3.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebertcisco/vercel-typescript-express-api.git"
  },
  "keywords": [
    "vercel-typescript-express-api",
    "node",
    "mongo",
    "express",
    "javascript",
    "server"
  ],
  "author": "Hebert F. Barros",
  "bugs": {
    "url": "https://github.com/hebertcisco/vercel-typescript-express-api/issues"
  },
  "homepage": "https://github.com/hebertcisco/vercel-typescript-express-api#readme"
}
